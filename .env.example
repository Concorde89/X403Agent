# Example environment variables file
# Copy this to .env and fill in your actual values
# NEVER commit .env file to git!

# Solana RPC Configuration
SOLANA_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY_HERE
TOKEN_MINT=YourTokenMintAddressHere

# Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_AUDIENCE=http://localhost:8000
ISSUER=bot-api-v1
TTL_SECONDS=60

# GitHub Configuration (for database publishing)
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_REPO_NAME=X403Agent
GITHUB_USERNAME=your_github_username
GIT_BRANCH=main
GIT_PUSH=true

# Database Configuration
DB_PATH=token_holders.db

# Bot Configuration
BOT_RUN_INTERVAL_MINUTES=5
